" cursor looks
let &t_SI = "\e[6 q" "Insert mode line
let &t_SR = "\e[1 q" "Replace mode blinking block
let &t_EI = "\e[2 q" "Normal mode block (technically when exiting insert or replace mode)


let mapleader = "\<Space>"

" diverse keymaps
nnoremap , ;
nnoremap ; ,
nnoremap ´ `
nnoremap ´´ ``
